{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ],
    "react/forbid-prop-types": 0,
    "import/no-duplicates": 0,
    "import/prefer-default-export": 0,
    "no-shadow": 0,
    "no-undef": 0,
    "consistent-return": 0,
    "react/jsx-props-no-spreading": 0
  }
}
